from math import *

if __name__ == '__main__':
    s = input().split()
    se = set()
    d = dict({})  
    for x in s:
        if x in d:
            d[x] += 1
        else:
            d[x] = 1
    for x in sorted(d):
        print(x, d[x])
    print()
    for x in d:
        print(x, d[x])


    
            
    
