from math import *
from functools import cmp_to_key
def cmp(a,b):
    if len(a) != len(b):
        if len(a) > len(b):
            return 1
        else:
            return -1
    else:
        if a > b:
            return 1
        elif a < b:
            return -1

if __name__ == '__main__':
    s = input().split()
    for x in sorted(s):
        print(x, end = ' ')
    print()
    t = sorted(s,key = cmp_to_key(cmp))
    for x in t:
        print(x, end = ' ')

    
            
    
