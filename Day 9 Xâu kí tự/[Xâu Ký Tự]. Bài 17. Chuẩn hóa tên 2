from math import *

if __name__ == '__main__':
    s = input().split()
    for i in range(0,len(s)):
        if i != len(s) - 1:
            if i < len(s) - 2:
                 print(s[i].title(), end = ' ')
            else:
                print(s[i].title(), end = '')
        else:
            print(',', s[i].upper(), end = '\n')
    print(s[len(s) - 1].upper(), ',', sep = '', end = ' ')
    for i in range(0,len(s) - 1):
           print(s[i].title(), end = ' ')

        

    
            
    
