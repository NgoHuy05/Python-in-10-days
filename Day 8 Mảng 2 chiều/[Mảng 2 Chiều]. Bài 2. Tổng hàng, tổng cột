from math import *

if __name__ == '__main__':
    n, m = map(int,input().split())
    a = []
    for i in range(n):
        b = list(map(int,input().split()))
        a.append(b)
    for i in range(n):
        sumh = 0
        for j in range(m):
            sumh += a[i][j]
        print(sumh, end = ' ')
    print()
    for j in range(m):
        sumc = 0
        for i in range(n):
            sumc += a[i][j]
        print(sumc, end = ' ')
    
