from math import *
from functools import cmp_to_key
def sum(n):
    tong = 0
    while n:
        tong += n % 10
        n //= 10
    return tong
def cmp(a,b):
    if sum(a) == sum(b):
        return a - b
    else:
        return sum(a) - sum(b)

if __name__ == '__main__':
    n = int(input())
    a = list(map(int,input().split()))
    a.sort(key = cmp_to_key(cmp))
    for x in a:
        print(x, end = ' ')
