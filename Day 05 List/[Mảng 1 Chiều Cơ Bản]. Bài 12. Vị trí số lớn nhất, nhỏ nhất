from math import *

if __name__ == '__main__':
    n = int(input())
    a = list(map(int,input().split()))
    min,max = 10e9,-10e9
    for x in a:
        if x > max:
            max = x
        if x < min:
            min = x
    for i in range(len(a) - 1, -1, -1):
        if a[i] == min:
            print(i, end = ' ')
            break
    for i in range(len(a)):
        if a[i] == max:
            print(i)
        

        

    
    
