from math import *
def check(a, n):
    l, r = 0, len(a) - 1
    while l <= r:
        if a[l] != a[r]:
            return 0
        l += 1
        r -= 1
    return 1

if __name__ == '__main__':
    n = int(input())
    a = list(map(int,input().split()))
    snt, stn, scp, tsnt = 0, 0, 0, 0      
    if check(a,n):
        print('YES')
    else:
        print('NO')
        

        

    
    
